version: '3.3'
services:
    dockupdater:
        container_name: dockupdater
        environment:
            - CLEANUP=true
            - LOG_LEVEL=info
            - INTERVAL=7200
            - LABEL=true
            - LATEST=true
            - WAIT=180
        volumes:
            - '/var/run/docker.sock:/var/run/docker.sock'
        image: dockupdater/dockupdater
